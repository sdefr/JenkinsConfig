pipeline { 
    agent any 
    stages {
        stage('clean') { 
            steps { 
                sh "maven clean"
            }
        }
        stage('compile and run')
        {
        steps{
        sh "javac Main.java"
        sh "Main java"
        }
        }
        stage('Test'){
            steps {
                sh 'mvn test'
                
            }
        }
        stage('Deploy') {
            steps {
                sh 'mvn clean package'
            }
        }
    }
}
