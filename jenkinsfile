pipeline { 
    agent any 
    stages {
        stage('clean') { 
            steps { 
               sh "mvn clean"
            }
        }
        stage('Test'){
            steps {
                sh 'mvn test'
             
            }
        }
        
        stage('Compile and Run'){
        steps{
        sh "javac Main.java"
        sh "Main java"
        echo "done"
        }
        }
        stage('build') {
            steps {
                sh 'mvn package'
            }
        }
    }
}
